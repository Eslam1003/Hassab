<!-- الاسم  -->
<div class="mb-3">
  <input
    class="form-control"
    name="name"
    id="name"
    placeholder="الاسم"
    required
    type="text"
    value="<%= visit.name %>"
  />
</div>

<!-- رقم الهاتف -->
<div class="input-group mb-3">
  <input
    class="form-control"
    placeholder="رقم الهاتف"
    name="phoneNum1"
    id="name"
    value="<%= visit.phoneNum1 %>"
    required
  />
  <span class="input-group-text">-</span>
  <input
    class="form-control"
    placeholder="رقم الارضي"
    name="phoneNum2"
    id="name"
    value="<%= visit.phoneNum2 %>"
    required
  />
</div>
<!-- العنوان -->
<div class="input-group mb-3">
  <input
    class="form-control"
    name="ragion"
    placeholder="المنطفة"
    id="ragion"
    type="text"
    value="<%= visit.ragion %>"
    required
  />
  <span class="input-group-text">-</span>
  <input
    class="form-control"
    placeholder="اسم الشارع"
    name="stname"
    id="stname"
    type="text"
    required
    value="<%= visit.stname %>"
  />
  <span class="input-group-text">-</span>
  <input
    class="form-control"
    placeholder="رقم العماره"
    name="blnum"
    id="blnum"
    type="text"
    required
    value="<%= visit.blnum %>"
  />
  <span class="input-group-text">-</span>
  <input
    class="form-control"
    placeholder="الدور"
    name="flnum"
    id="flnum"
    type="text"
    required
    value="<%= visit.flnum %>"
  />
  <span class="input-group-text">-</span>
  <input
    class="form-control"
    placeholder="رقم الشقه"
    name="donum"
    id="donum"
    type="text"
    required
    value="<%= visit.donum %>"
  />
  <span class="input-group-text">-</span>
  <input
    class="form-control"
    placeholder="علامه مميزه"
    name="spchilmark"
    id="spchilmark"
    type="text"
    required
    value="<%= visit.spchilmark %>"
  />
</div>

<!-- التحاليل -->
<div class="input-group mb-3">
  <span class="input-group-text mb-3">اسماء التحاليل</span>
  <div id="test" class="form-control mb-3" contenteditable="true">
    <%- visit.tests %>
  </div>
  <textarea
    style="display: none"
    class="form-control mb-3"
    aria-label="With textarea"
    name="tests"
    id="tests"
    required
  >
  <%= visit.tests %>
  </textarea>
</div>

<!-- الوقت  -->
<div class="input-group mb-3">
  <select class="form-select" aria-label="Default select example" name="time">
    <option selected><%= visit.time %></option>
    <option value="a-8:00-8:30">8:00-8:30</option>
    <option value="a-8:30-9:00">8:00-8:00</option>
    <option value="a-8:00-9:00">8:00-9:00</option>

    <option value="c-9:00-9:30">9:00-9:30</option>
    <option value="c-9:30-10:00">9:30-10:00</option>
    <option value="c-9:00-10:00">9:00-10:00</option>

    <option value="e-10:00-10:30">10:00-10:30</option>
    <option value="e-10:30-11:00">10:30-11:00</option>
    <option value="e-10:00-11:00">10:00-11:00</option>

    <option value="d-11:00-11:30">11:00-11:30</option>
    <option value="d-11:30-12:00">11:30-12:00</option>
    <option value="d-11:00-12:00">11:00-12:00</option>

    <option value="e-12:00-12:30">12:00-12:30</option>
    <option value="e-12:30-12:00">12:30-12:00</option>
    <option value="e-12:00-1:00">12:00-1:00</option>

    <option value="f-1:00-1:30">1:00-1:30</option>
    <option value="f-1:30-2:00">1:30-2:00</option>
    <option value="f-1:00-2:00">1:00-2:00</option>

    <option value="g-2:00-2:30">2:00-2:30</option>
    <option value="h-3:00-3:30">3:00-3:30</option>
    <option value="j-4:00-4:30">4:00-4:30</option>
    <option value="k-5:00-5:30">5:00-5:30</option>
    <option value="l-6:00-6:30">6:00-6:30</option>
    <option value="m-7:00-7:30">7:00-7:30</option>
  </select>

  <span class="input-group-text">-</span>
  <input
    class="form-control mp-3"
    name="date"
    id="date"
    type="date"
    required
    value="<%= visit.date %>"
  />
  <span class="input-group-text">-</span>
  <input
    class="form-control mp-3"
    name="creater"
    id="creater"
    type="text"
    name="creater"
    placeholder="مستقبل الزياره"
    value="<%= visit.creater %>"
    required
  />
  <select name="chimist" class="form-select" required id="">
    <option selected><%= visit.chimist %></option>
    <% chimistName.forEach(chimist => { %>
    <option value="<%= chimist %>"><%= chimist %></option>
    <% }) %>
  </select>
</div>
<div class="input-group mb-3">
  <span class="input-group-text mb-3">ملاحظات</span>
  <textarea class="form-control mb-3" name="nots" id="nots" required>
<%= visit.nots %></textarea
  >
</div>
<div class="input-group mb-3">
  <span class="input-group-text">الاجمالي</span>
  <input
    class="form-control"
    placeholder="اجمالي التكلفه"
    name="cost"
    id="cost"
    type="text"
    required
    value="<%= visit.cost %>"
  />
  <span class="input-group-text">عدد الافراد الاضافين</span>
  <input
    class="form-control"
    placeholder="عدد الافراد الاضافين"
    name="num"
    id="num"
    type="number"
    value="<%= visit.num %>"
  />
</div>
<div class="form-check form-check-inline">
  <input
    class="form-check-input"
    type="radio"
    name="stat"
    id="inlineRadio1"
    value="red"
  />
  <label class="form-check-label" for="inlineRadio1">Del</label>
</div>
<div class="form-check form-check-inline">
  <input
    class="form-check-input"
    type="radio"
    name="stat"
    id="inlineRadio2"
    value="yellow"
    required
  />
  <label class="form-check-label" for="inlineRadio2">In Call</label>
</div>
<div class="form-check form-check-inline">
  <input
    class="form-check-input"
    type="radio"
    name="stat"
    id="inlineRadio3"
    value="white"
    required
  />
  <label class="form-check-label" for="inlineRadio3">Normal</label>
</div>

<div class="form-check form-check-inline">
  <input
    class="form-check-input"
    type="radio"
    name="stat"
    id="inlineRadio5"
    value="pending"
    required
  />
  <label class="form-check-label" for="inlineRadio5">Pending</label>
</div>
